//1
#include <stdio.h>
int divide(int num,int count);
int main(){
    int n=15,count=0;
    divide(n,count);
}
int divide(int num,int count){
    if (num==1){
        printf("Number of times array can be halfed is : %d",count);
        return 0;
    }
    else{
        int num2=num/2;
        count+=1;
        num=num2+divide(num2,count);
        return (count); 
    }
}


//2

#include <stdio.h>

int main() {
    char s[500];
    gets(s);
    int upper=0,lower=0,white_space=0,digits=0,special=0;
    int i=0;
    while(s[i] != '\0')
    {
        if (s[i]>='A' && s[i]<='Z')
        {
            upper++;
        }
        else if(s[i]>='a' && s[i]<='z')
        {
            lower++;
        }
        else if(s[i]>='0' && s[i]<='9')
        {
            digits++;
        }
        else if(s[i] == ' ')
        {
            white_space++;
        }
        else
        {
            special++;
        }
        ++i;
    }
    printf("Uppercase letters %d\n",upper);
    printf("Lowercase letters %d\n",lower);
    printf("Digits %d\n",digits);
    printf("White spaces %d\n",white_space);
    printf("Special Characters %d\n",special);
    return 0;
}

//3
#include <stdio.h>
int fact(int num);
int sos(int num);
int main(){
    int n;
    scanf("%d",&n);
    printf("The sum of series is : %d",sos(n));
}
int fact(int num){
    if (num==1){
        return 1;
    }
    else{
        return num*fact(num-1);
    }
}
int sos(int num){
    if(num==1){
        return 1;
    }
    else{
        return (fact(num)/num)+sos(num-1);
    }
}

//4
#include <stdio.h>
#include<string.h>
int main(){
    char type[100];
    scanf("%s",type);
    if (strcmp(type,"Hatchback") ==0||strcmp(type,"Sedan") ==0||strcmp(type,"SUV") ==0||strcmp(type,"MUV") ==0){
        int car,extra,discount,net;
        scanf("%d",&car);
        scanf("%d",&extra);
        int total=car+extra;
        if(strcmp(type,"Hatchback") ==0){
            discount=0.03*total;
        }
        if(strcmp(type,"Sedan") ==0){
            discount=0.05*total;
        }
        if(strcmp(type,"SUV") ==0){
            discount=0.10*total;
        }
        if(strcmp(type,"MUV") ==0){
            discount=0.15*total;
        }
        net=total + discount+ (total-discount)*0.12;
        printf("The On-road price of the car is: %d",net);
    }
    else{
        printf("Invalid Type");
    }
}

//5
#include <stdio.h>
#include<string.h>
#include<ctype.h>
int main(){
    char string[100];
    fgets(string,100,stdin);
    int n=0;
    for (int i=0;i<strlen(string);i++){
        if (i==0){
           printf("%c",toupper(string[i])); 
        }
        else{
            if(n==0){
                printf("%c",tolower(string[i]));
            }
        }
        if(n==1){
            printf("%c",toupper(string[i]));
        }
        if (string[i]==' '){
            n=1;
        }
        
    }
    if (strlen(string)<20){
        printf("The length of string is: %d",strlen(string));
    }
    else{
        printf("%d",100);
    }
    
    
}
