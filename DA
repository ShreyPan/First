//1
#include <stdio.h>

int ifprime(int num1);
void printprime(int num11);
int ifarm(int num2);
void printarm(int num22);
int ifper(int num3);
void printper(int num33);

int main() 
{
    int a;
    scanf("%d", &a);
    printprime(a);
    printf("\n");
    printarm(a);
    printf("\n");
    printper(a);
    return 0;
}

void printprime(int num11)
{
    if (ifprime(num11))
    {
        printf("%d is a prime number", num11);
    }
    else
    {
        printf("%d is not a prime number", num11);
    }
}
int ifprime(int num1)
{
    int i;
    for (i=2; i<num1; i++)
    {
        if (num1%i==0)
        {
            return 0;
        }
    }
    return 1;
}

void printarm(int num22)
{
    if (ifarm(num22))
    {
        printf("%d is a armstrong number", num22);
    }
    else
    {
        printf("%d is not a armstrong number", num22);
    }
}

int ifarm(int num2)
{
    int r, sum=0, temp;
    for(temp=num2; num2!=0; num2=num2/10)
    {
        r=num2%10;
        sum=sum+(r*r*r);
    }
    if (sum=temp)
    {
        return 0;
    }
    return 1;
}

void printper(int num33)
{
    if (ifper(num33))
    {
        printf("%d is a perfect number", num33);
    }
    else
    {
        printf("%d is not a perfect number", num33);
    }
}

int ifper(int num3)
{
    int i, sum=0;
    for (i=1; i<num3; i++)
    {
        if (num3%i==0)
        {
            sum=sum+1;
        }
    }
    if (num3=sum)
    {
        return 0;
    }
    return 1;
}

//2

#include <stdio.h>
#include <string.h>
#include <ctype.h>
int main(){
    char string[100];
    int upper=0,lower=0,digit=0,whitespace=0,special=0;
    fgets(string,100,stdin);
    for(int i=0;i<strlen(string);i++){
        if (isupper(string[i])){
            upper+=1;
        }
        if (islower(string[i])){
            lower+=1;
        }
        if (isdigit(string[i])){
            digit+=1;
        }
        if (string[i]==' '){
            whitespace+=1;
        }
        if (ispunct(string[i])){
            special+=1;
        }
    }
    printf("Number of uppercase : %d\n",upper);
    printf("Number of lowercase : %d\n",lower);
    printf("Number of digits : %d\n",digit);
    printf("Number of whitespace : %d\n",whitespace);
    printf("Number of special characters : %d\n",special);
}

//3
#include <stdio.h>

int main() {
    char s[200];
    scanf("%s",s);
    int l=strlen(s);
    int j,i=0,a=l;
    int f=0;
    char a1;
    printf("Length of the string is %d\n",l);
    for(i=0;i<l;i++)
    {
        for(j=i+1;j<l;++j)
        {
            if (s[i] == s[j])
            {
                a=a-1;
            }
            if (s[i] == s[j] && f==0)
            {
                f=1;
                a1=s[i];
            }
        }
    }
    printf("Word frequency is %d\n",a);
    if (f ==0)
    {
        printf("No character is repeated in the string");
    }
    else
    {
        printf("First repeated character is %c\n",a1);
    }
    for(i=0;i<l;i++)
    {
        f=0;
        for(j=i+1;j<l;++j)
        {
            if (s[i] == s[j])
            {
                f=1;
            }
        }
        if (f==0)
        {
            printf("First non-repeated character is %c",s[i]);
            break;
        }
    }
    
    return 0;
}

//4

#include<stdio.h>
#include <string.h>
struct office {
   char name[20];
   int age;
   char position[20];
   char date[50];
} e[10], temp;
 
void main() {
   int i, j, n;
   scanf("%d",&n);
   for (i = 0; i < 3; i++) {
      scanf("%s", e[i].name);
      scanf("%d",& e[i].age);
      scanf("%s", e[i].position);
      scanf("%s", e[i].date);
   }
   for (i = 0; i < n-1; i++){
        if (strcmp(e[i].name, e[i + 1].name) > 0) {
            temp = e[i];
            e[i] = e[i + 1];
            e[i + 1] = temp;
        }
            
    }
 
   for (i = 0; i < n; i++) {
      printf("%s\n",e[i].name);
      printf("%d\n",e[i].age);
      printf("%s\n",e[i].position);
      printf("%s\n",e[i].date);
   }
   for (i = 0; i < n-1; i++){
        if (strcmp(e[i].date, e[i + 1].date) > 0) {
            temp = e[i];
            e[i] = e[i + 1];
            e[i + 1] = temp;
        }
            
    }
 
   for (i = 0; i < n; i++) {
      printf("%s\n",e[i].name);
      printf("%d\n",e[i].age);
      printf("%s\n",e[i].position);
      printf("%s\n",e[i].date);
   }
}

