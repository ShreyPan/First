//1
#include <stdio.h>
int main(){
    int temp[10];
    for(int i=0;i<7;i++){
        printf("Day %d temperature in Channai : ",i+1);
        scanf("%d",&temp[i]);
    }
    for(int i=0;i<7;i++){
        printf("Day %d temperature in Gangtok : %d\n",i+1,(temp[i]+5)-(temp[i]-8));
    }
}

//2
#include <stdio.h>
int intsum(int n);
int intlen(int n);
int main() {
    int num=0;
  for(int i=1000;i<10000;i++){
      if (i%2==0){
          num+=i;
      }
  }
   while(intlen(intsum(num))!=1){
       num=intsum(num);
   }
   if(intsum(num)%2==0){
       printf("Even found");
   }
   else{
       printf("Odd found");
   }
}
int intsum(int n){
    int sum=0;
    int left;
    while(n!=0){
        left=n%10;
        n=n/10;
        sum+=left;
    }
    return sum;
}
int intlen(int n){
    int count=0;
    while(n!=0){
        n=n/10;
        count+=1;
    }
    return count;
}

//3
#include <stdio.h>
#include <string.h>
int main() {
    char pass[3][3];
    char password[6];
    char stored[6]="aeiceg";
    for (int i = 0; i < 3; i++) {
        for (int j = 0; j < 3; j++) {
            scanf(" %c", &pass[i][j]);
        }
    }
    password[0] = pass[0][0];
    password[1] = pass[1][1];
    password[2] = pass[2][2];
    password[3] = pass[0][2];
    password[4] = pass[1][1];
    password[5] = pass[2][0];
    printf("%s\n",password);
    if(strcmp(password,stored)==0){
        printf("Correct password");
    }
    else{
        printf("Incorrect password");
    }
    return 0;
}

//4
#include <stdio.h>
int avg(int n[25]);
int main(){
    int marks[25];
    int month[25];
    int marks2[25];
    for(int i=0;i<25;i++){
        scanf("%d",&marks[i]);
        scanf("%d",&month[i]);
    }
    printf("Class average before : %d\n",avg(marks));
    for(int i=0;i<25;i++){
        marks2[i]=marks[i]+month[i];
    }
    printf("Class average after : %d\n",avg(marks2));
    if(avg(marks2)-avg(marks)>=5){
        printf("Can implement - Significant increase in class average.");
    }
    else{
        printf("Need not implement - No significant increase in class average.");
    }
    
}
int avg(int n[25]){
    int sum=0;
    for (int i=0;i<25;i++){
        sum+=n[i];
    }
    return sum/25;
}



//5
#include <stdio.h>
#include <stdlib.h>
int power(int x,int n);
int main(){
    int n,x;
    scanf("%d",&x);
    scanf("%d",&n);
    if(-n!=abs(n)){
        printf("%d",power(x,n));
    }
    else{
        float z=power(x,abs(n));
        printf("%f",1/z);
    }
}
int power(int x,int n){
    if(n==0){
        return 1;
    }
    n--;
    return (x*power(x,n));
}

//triangles

// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int wrong=0,right=0,acute=0,obtuse=0;
    int arr[100];
    int n=5;
    for (int i=1;i<n+1;i++){
        int sum=0;
        for(int j=0;j<3;j++){
            scanf("%d",&arr[j]);
        }
        int a=arr[0],b=arr[1],c=arr[2];
        sum=a+b+c;
        if (sum==180){
            if(a==90||b==90||c==90){
                right+=1;
            }
            if(a>90||b>90||c>90){
                obtuse+=1;
            }
            if(a<90&&b<90&&c<90){
                acute+=1;
            }
        }
        else{
            printf("Wrong Entry Try again\n");
            wrong+=1;
            n+=1;
            continue;
        }
    }
    printf("Acute Angled triangle: %d\n",acute);
    printf("Right Angled triangle: %d\n",right);
    printf("Obtuse Angled triangle: %d\n",obtuse);
    printf("Wrong Enteries: %d\n",wrong);
}


